    "version": "0.2.0",
    "configurations": [

        {
            "name": "chrome",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug",
            "args": ["-d", "chrome","--web-port", "8080"],
            
        },
        {
            "name": "chrome-disable-web-sec",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug",
            "args": ["-d", "chrome","--web-port", "8080", "--web-browser-flag","--disable-web-security"],
        },        
        {
            "name": "web-server",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug",
            "args": ["-d", "web-server","--web-port", "8080"],
        },
        {
            "name": "mobile",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug",
        },

    ]
}